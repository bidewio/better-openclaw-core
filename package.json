{
  "name": "@better-openclaw/core",
  "version": "1.0.15",
  "private": false,
  "description": "Core logic for better-openclaw: schemas, service registry, resolver, composer, validators and more",
  "packageManager": "pnpm@9.15.4",
  "sideEffects": false,
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "default": "./dist/*.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:tsc": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/node": "^25.2.2",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}